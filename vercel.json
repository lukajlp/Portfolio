{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "install": {
    "command": "npm install && npm install bootstrap@5.3.0-alpha3 --save",
    "include": [
      "node_modules/**"
    ]
  },
  "functions": { "api/*.js": { "memory": 128 } },
  "cleanUrls": true
}
